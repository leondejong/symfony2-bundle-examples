<!DOCTYPE html>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<title>
		{% block title %}{% endblock %}
		</title>
		{% block stylesheets %}
        {% stylesheets filter='cssrewrite' filter='?yui_css' output='css/compiled/compiled.css'
            'bundles/exampleapplication/css/reset.css'
            'bundles/exampleapplication/css/style.css'
        %}
		<link rel="stylesheet" type="text/css" href="{{ asset_url }}" media="screen" />
        {% endstylesheets %}
		{% endblock %}
		<link rel="shortcut icon" href="{{ asset('bundles/exampleapplication/graphics/icon.png') }}" />
	</head>
	<body>
		{% block body %}{% endblock %}
		{% block javascripts %}
        {% javascripts filter='?yui_js' output='js/compiled/compiled.js'
            '@ExampleApplicationBundle/Resources/public/js/jquery-1.8.2.min.js'
            '@ExampleApplicationBundle/Resources/public/js/utilities.js'
            '@ExampleApplicationBundle/Resources/public/js/functions.js'
        %}
		<script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
		{% endblock %}
    </body>
</html>