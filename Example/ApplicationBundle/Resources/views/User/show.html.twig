{% extends 'ExampleApplicationBundle:Layout:main.html.twig' %}
{% block header %}
<h1>Show User</h1>
{% endblock header %}
{% block local %}
{% include "ExampleApplicationBundle:User:navigation.html.twig" %}
{% endblock local %}
{% block content %}
{% block read %}
<table class="read">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Type</th>
            <td>{{ entity.type }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ entity.status }}</td>
        </tr>
        <tr>
            <th>Position</th>
            <td>{{ entity.position }}</td>
        </tr>
        <tr>
            <th>Data</th>
            <td>{{ entity.data }}</td>
        </tr>
        <tr>
            <th>Date</th>
            <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Change</th>
            <td>{{ entity.change|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Username</th>
            <td>{{ entity.username }}</td>
        </tr>
        <tr>
            <th>Password</th>
            <td>{{ entity.password }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Level</th>
            <td>{{ entity.level }}</td>
        </tr>
        <tr>
            <th>Views</th>
            <td>{{ entity.views }}</td>
        </tr>
        <tr>
            <th>Logins</th>
            <td>{{ entity.logins }}</td>
        </tr>
        <tr>
            <th>Success</th>
            <td>{{ entity.success }}</td>
        </tr>
        <tr>
            <th>Failure</th>
            <td>{{ entity.failure }}</td>
        </tr>
        <tr>
            <th>Marks</th>
            <td>{{ entity.marks }}</td>
        </tr>
        <tr>
            <th>Key</th>
            <td>{{ entity.key }}</td>
        </tr>
        <tr>
            <th>Session</th>
            <td>{{ entity.session }}</td>
        </tr>
        <tr>
            <th>Cookie</th>
            <td>{{ entity.cookie }}</td>
        </tr>
        <tr>
            <th>Ip</th>
            <td>{{ entity.ip }}</td>
        </tr>
        <tr>
            <th>Agent</th>
            <td>{{ entity.agent }}</td>
        </tr>
        <tr>
            <th>Accept</th>
            <td>{{ entity.accept }}</td>
        </tr>
        <tr>
            <th>Charset</th>
            <td>{{ entity.charset }}</td>
        </tr>
        <tr>
            <th>Encoding</th>
            <td>{{ entity.encoding }}</td>
        </tr>
        <tr>
            <th>Language</th>
            <td>{{ entity.language }}</td>
        </tr>
        <tr>
            <th>Visit</th>
            <td>{{ entity.visit|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Profile</th>
            {% if entity.profile is empty %}
            <td>-</td>
            {% else %}
            <td>{{ entity.profile.title }}</td>
            {% endif %}
        </tr>
        <tr>
            <th>Pages</th>
            {% if entity.pages is empty %}
            <td>-</td>
            {% else %}
            <td>{% for page in entity.pages %}{{ page.title }}{% if not loop.last %}, {% endif %}{% endfor %}</td>
            {% endif %}
        </tr>
        <tr>
            <th>Items</th>
            {% if entity.items is empty %}
            <td>-</td>
            {% else %}
            <td>{% for item in entity.items %}{{ item.title }}{% if not loop.last %}, {% endif %}{% endfor %}</td>
            {% endif %}
        </tr>
        <tr>
            <th>Objects</th>
            {% if entity.objects is empty %}
            <td>-</td>
            {% else %}
            <td>{% for object in entity.objects %}{{ object.title }}{% if not loop.last %}, {% endif %}{% endfor %}</td>
            {% endif %}
        </tr>
        <tr>
            <th>Messages</th>
            {% if entity.messages is empty %}
            <td>-</td>
            {% else %}
            <td>{% for message in entity.messages %}{{ message.title }}{% if not loop.last %}, {% endif %}{% endfor %}</td>
            {% endif %}
        </tr>
    </tbody>
</table>
{% endblock read %}
{% block update %}
<div class="update">
    <a href="{{ path('user_edit', { 'id': entity.id }) }}">
        Edit User
    </a>
</div>
{% endblock update %}
{% block delete %}
<form action="{{ path('user_delete', { 'id': entity.id }) }}" method="post" class="delete" novalidate>
    {{ form_widget(delete_form) }}
    <button type="submit">Delete User</button>
</form>
{% endblock delete %}
{% endblock content %}